
<%# <div class="flex flex-wrap gap-4 justify-center"> %>
<div>
  <div class="flex justify-center items-center navbar navbar-expand-sm navbar-light navbar-lewagon fixed top-16 shadow-sm">
    <div class="px-4 my-3">
      <p class="text-2xl">
        <i class="fas fa-star"></i>
      </p>
    </div>
    <div class="px-4 my-3">
      <p class="text-2xl">
        <i class="fas fa-hamburger"></i>
      </p>
    </div>
    <div class="px-4 my-3">
      <p class="text-2xl">
        <i class="fas fa-fish"></i>
      </p>
    </div>
    <div class="px-4 my-3">
      <p class="text-2xl">
        <i class="fas fa-pepper-hot"></i>
      </p>
    </div>
    <div class="px-4 my-3">
      <p class="text-2xl">
        <i class="fa-solid fa-bowl-food"></i>
      </p>
    </div>
    <div class="px-4 my-3">
      <p class="text-2xl">
        <i class="fa-solid fa-martini-glass-citrus"></i>
      </p>
    </div>
    <div class="px-4 my-3">
      <p class="text-2xl">
        <i class="fas fa-carrot"></i>
      </p>
    </div>

  </div>

  <div class="show-container">
  <div class="pt-10">
    <h1 class="text-5xl">Visits</h1>
    <h2 class="text-4xl">Upcoming reservation</h2>
    <div class="flex">
      <div class="mr-20">
          <img src="https://placeimg.com/200/180/arch" class="rounded-2xl" />
        </div>
      <div>
        <h1 class="font-semibold text-2xl flex">
        <% @current_bookings.each do |booking| %>
        <%= booking.restaurant.name %>
        <%= booking.booking %>
        <% end %>
        </h1>
      </div>
      
  </div>
